{
  "Rsyslog": {
    "display"   : "Switch",
    "path"      : "/var/log/rsyslog.log",
    "format"    : {
      "multiline": "Error",
      "regex"    : "|([0-9]{4,4}-[0-9]{1,2}-[0-9]{1,2})T([0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}) switch.*? (.*?) (.*?)$|",
                "match"    : {
                        "Date"    : 1,
                        "Time"    : 2,
                        "Client"  : 3,
                        "Message" : 4
                        },
                "types"    : {
                        "Date"    : "date:Y-m-d",
                        "Time"    : "date:H:i:s",
                        "Client"  : "txt",
                        "Message" : "txt"
                    }
    }
  }
}
