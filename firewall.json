{
  "Rsyslog": {
    "display"   : "Router Firewall",
    "path"      : "/var/log/rsyslog.log",
    "format"    : {
      "multiline": "Error",
      "regex"    : "|([0-9]{4,4}-[0-9]{1,2}-[0-9]{1,2})T([0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2}) .*? kernel: (.*?)IN=(.*?) .*SRC=(.*?) .*PROTO=(.*?) .*SPT=(.*?) .*DPT=(.*?) .*? .*? (.*?)$|",
                "match"    : {
                        "Date"    : 1,
                        "Time"    : 2,
                        "FW Rule" : 3,
			"IN"      : 4,
			"SRC"     : 5,
			"PROTO"   : 6,
			"SPT"     : 7,
			"DPT"     : 8,
			"FLAG"    : 9
                        },
                "types"    : {
                        "Date"    : "date:Y-m-d",
                        "Time"    : "date:H:i:s",
                        "FW Rule" : "txt",
			"IN"	  : "txt",
			"SRC"     : "txt",
			"PROTO"   : "txt",
			"SPT"     : "txt",
			"DPT"     : "txt",
			"FLAG"    : "txt"
                    }
    }
  }
}
